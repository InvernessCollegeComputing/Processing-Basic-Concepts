// this is our stickman function

// note that our function name starts with a lower case letter
// this is good practice

void drawStickMan(){    
  
  fill(0);
  ellipseMode(CENTER);
  
  ellipse(500, 250, 50, 50);
  line(500, 275, 500, 350);
  line(450, 260, 500, 300);
  line(500, 300, 550, 260);
  line(470, 425, 500, 350);
  line(500, 350, 530, 425); 
  
  
  
}
